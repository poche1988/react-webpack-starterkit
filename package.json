{
	"name": "react-webpack-starterkit",
	"version": "1.0.0",
	"description": "React Webpack Starterkit",
	"main": "index.js",
	"scripts": {
		"start": "npm run start:local",
		"start:local": "npm run flow & webpack-dev-server --config config/webpack.common.js --env.PLATFORM=local",
		"start:dev": "npm run flow & webpack-dev-server --config config/webpack.dev.js",
		"start:prod": "npm run flow & npm run build:local",
		"prebuild:local": "webpack --config config/webpack.prod.js --progress",
		"build:local": "node script/server.js",
		"build": "npm run flow & webpack --config config/webpack.prod.js --progress",
		"size:dev": "npm run flow & webpack --config config/webpack.dev.js --env.STAT=1 --progress",
		"size:prod": "npm run flow & webpack --config config/webpack.prod.js --env.STAT=1 --progress",
		"lint": "npm run flow & eslint src/**/*.js src/**/*.jsx",
		"flow": "flow",
		"test": "jest --watchAll",
		"postinstall": "flow-typed install -s"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/thomas-lg/react-webpack-starterkit"
	},
	"keywords": [
		"react",
		"starterkit",
		"webpack",
		"react-router"
	],
	"author": "TLG",
	"license": "MIT",
	"dependencies": {
		"axios": "^0.18.0",
		"formik": "^1.5.1",
		"normalizr": "^3.3.0",
		"react": "^16.8.4",
		"react-dom": "^16.8.4",
		"react-router-dom": "^4.3.1"
	},
	"devDependencies": {
		"@babel/core": "^7.3.4",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/preset-env": "^7.3.4",
		"@babel/preset-flow": "^7.0.0",
		"@babel/preset-react": "^7.0.0",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.0.1",
		"babel-jest": "^24.5.0",
		"babel-loader": "^8.0.5",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"clean-webpack-plugin": "^1.0.1",
		"css-loader": "^1.0.1",
		"eslint": "^5.15.1",
		"eslint-config-prettier": "^3.6.0",
		"eslint-loader": "^2.1.2",
		"eslint-plugin-flowtype": "^3.4.2",
		"eslint-plugin-flowtype-errors": "^3.6.0",
		"eslint-plugin-prettier": "^3.0.1",
		"eslint-plugin-react": "^7.12.4",
		"express": "^4.16.4",
		"file-loader": "^2.0.0",
		"flow-bin": "^0.87.0",
		"flow-typed": "^2.5.1",
		"html-webpack-plugin": "^3.2.0",
		"jest": "^24.5.0",
		"mini-css-extract-plugin": "^0.4.4",
		"node-sass": "^4.11.0",
		"optimize-css-assets-webpack-plugin": "^5.0.1",
		"prettier": "^1.16.4",
		"react-hot-loader": "^4.8.0",
		"react-test-renderer": "^16.8.4",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.1",
		"uglifyjs-webpack-plugin": "^2.1.2",
		"url-loader": "^1.1.2",
		"webpack": "^4.29.6",
		"webpack-bundle-analyzer": "^3.1.0",
		"webpack-cli": "^3.2.3",
		"webpack-dev-server": "^3.2.1",
		"webpack-merge": "^4.2.1"
	}
}
